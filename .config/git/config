[init]
  defaultBranch = master
[commit]
  gpgsign = true
[gpg]
	format = ssh
[core]
	excludesfile = ~/.gitignore
  whitespace = trailing-space,space-before-tab
[submodule]
	recurse = true
[color]
  ui = true
[push]
  default = current
[apply]
  whitespace = fix
[include]
  path = ~/.config/git/workspace.inc
