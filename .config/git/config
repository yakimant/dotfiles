[init]
  defaultBranch = master
[checkout]
  defaultRemote = origin
[commit]
  gpgsign = true
[gpg]
  format = ssh
[core]
  whitespace = trailing-space,space-before-tab
  hooksPath = ~/.config/git/hooks
[submodule]
  recurse = true
  fetchJobs = 8
[fetch]
  recurseSubmodules = on-demand
[color]
  ui = true
[push]
  default = current
[apply]
  whitespace = fix
[user]
  useConfigOnly = true
[http]
  maxRequests = 8
[include]
  path = ~/.config/git/workspace.inc
